{
  "expo": {
    "name": "Collector",
    "slug": "Collector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.polygonagro.Collector",
      "permissions": [
        "CAMERA",
        "PHOTO_LIBRARY",
        "NOTIFICATIONS",
        "MICROPHONE",
        "CALENDAR",
        "CONTACTS",
        "MOTION",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_STATE",
        "ACCESS_NETWORK_STATE",
        "CHANGE_NETWORK_STATE",
        "VIBRATE",
        "USE_FINGERPRINT",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#21202b"
      },
      "versionCode": 2,
      "package": "com.polygonagro.Collector",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "VIBRATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "RECEIVE_BOOT_COMPLETED",
        "USE_FINGERPRINT",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_STATE",
        "ACCESS_NETWORK_STATE",
        "CHANGE_NETWORK_STATE",
        "SYSTEM_ALERT_WINDOW",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "API_BASE_URL": "https://ag-world-orginal.vercel.app/",
      "SHOUTOUT_API_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..ayaQjSjBxcSSnqskZp_F_NlrLa_98ddiOi1lfK8WrJ4",
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/favicon.png",
          "color": "#21202b"
        }
      ],
      "expo-font",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo-camera": {
      "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
      "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
      "recordAudioAndroid": true
    },
    "newArchEnabled": true,
    "owner": ""
  }
}
